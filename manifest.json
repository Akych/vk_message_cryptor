{
    "name": "Vk message cryptor",
    "description" : "111",
    "version": "1",
    "manifest_version": 2,
    "browser_action": {
      "default_icon": "icon.png",
      "default_title": "shits",
      "default_popup":"./popup_menu/popup.html"
    },
    "content_scripts": [
        {
          "matches": ["https://vk.com/*"],
          "js": ["./scripts/crypt.js","./scripts/jquery.js","./scripts/index.js"],
          "run_art": "document_end"
        }
    ],
    "permissions": [
        "activeTab",
        "storage",
        "https://vk.com/*"
    ],
    "web_accessible_resources": [
      "i/*"
    ]
}